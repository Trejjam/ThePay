{
	"name": "contributte/thepay",
	"description": "ThePay library - nette extension",
	"keywords": [
		"the pay",
		"nette",
		"extension"
	],
	"license": [
		"MIT"
	],
	"require": {
		"php": ">=7.1",
		"contributte/thepay-lib": "^4.0.0",
		"trejjam/base-extension": "~0.7",
		"nette/di": "~2.3 || ~3.0",
		"nette/application": "~2.3",

		"ext-soap": "*"
	},
	"require-dev": {
		"nette/tester": "@dev",
		"nette/bootstrap": "@dev",
		"nette/robot-loader": "@dev",
		"tracy/tracy": "~2.5.0",

		"phpstan/phpstan": "~0.10.2",
		"symplify/easy-coding-standard": "~4.6.0"
	},
	"autoload": {
		"psr-4": {
			"Contributte\\ThePay\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/src/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "v4.0.x-dev"
		}
	}
}
